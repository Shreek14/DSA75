s1 = []
s = s.lower()
print(s)
for i in s:
    if 97 <= ord(i) <= 122 or 48 <= ord(i) <= 57:
        s1.append(i)

s1 = "".join(s1)
length = len(s1)
if length == 1:
    return True
if s1 == s1[::-1]:
    return True
else:
    return False
